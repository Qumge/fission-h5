<view class="tui-safe-area"><view class="tui-address"><block wx:for="{{addresses}}" wx:for-item="address" wx:for-index="index" wx:key="index"><block><tui-list-cell vue-id="{{'1-'+index}}" padding="0" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-address-flex"><view data-event-opts="{{[['tap',[['selectAddress',['$0'],[[['addresses','',index,'id']]]]]]]}}" class="tui-address-left" bindtap="__e"><view class="tui-address-main"><view class="tui-address-name tui-ellipsis">{{address.name}}</view><view class="tui-address-tel">{{address.phone}}</view></view><view class="tui-address-detail"><block wx:if="{{address.tag==='default'}}"><view class="tui-address-label">默认</view></block><text>{{address.content}}</text></view></view><view class="tui-address-imgbox" data-id="{{address.id}}" data-event-opts="{{[['tap',[['editAddress',['$0'],[[['addresses','',index,'id']]]]]]]}}" bindtap="__e"><image class="tui-address-img" src="/static/images/mall/my/icon_addr_edit.png"></image></view></view></tui-list-cell></block></block></view><view class="tui-address-new"><tui-button vue-id="2" type="danger" height="88rpx" data-event-opts="{{[['^click',[['newAddress']]]]}}" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">+ 新增收货地址</tui-button></view></view>